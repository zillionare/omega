[run]
omit =
    */sanity.py
    */cli.py
    */config/*
    */sync_funds.py
    omega/worker/__main__.py
    omega/master/__main__.py
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    def main
